
<form ng-submit="saveArticle()" name="newArticle">
	<h1 class="text-center">Nuevo Articulo</h1>
  <div class="alert alert-danger"></div>

  <div class="form-group">
    <label for="title">Titulo</label>
    <input type="text" name="title" placeholder="Titulo" ng-model="title">
  </div>
 
  <div class="form-group">
    <label for="content">Contenido</label>
    <input type="text" name="content" placeholder="Contenido" ng-model="content">
  </div>

  <div class="form-group">
    <input type="file" name="file" uploader-model="file">
  </div>     

  <div class="form-group">
    <input type="submit" value="Guardar">
  </div>
</form>



