
<form ng-submit="saveArticle(article)" name="newArticle">
	<h1 class="text-center">Nuevo Articulo</h1>
  <div class="alert alert-danger"></div>

  <div class="form-group">
    <label for="title">Titulo</label>
    <input type="text" name="title" placeholder="Titulo" ng-model="article.title">
  </div>
 
 <div class="form-group">
    <label for="content">Contenido</label>
    <input type="text" name="content" placeholder="COntenido" ng-model="article.content"></div>

  <div class="form-group">
    <label for="extension">Imagen</label>
    <input type="file" name="extension" ng-model="article.extension">
  </div>

 <p>
    <input type="submit" value="Guardar">
 </p>
</form>



