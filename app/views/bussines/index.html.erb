<h1>Listing bussines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bussines.each do |bussine| %>
      <tr>
        <td><%= bussine.name %></td>
        <td><%= bussine.address %></td>
        <td><%= bussine.latitude %></td>
        <td><%= bussine.longitude %></td>
        <td><%= link_to 'Show', bussine %></td>
        <td><%= link_to 'Edit', edit_bussine_path(bussine) %></td>
        <td><%= link_to 'Destroy', bussine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bussine', new_bussine_path %>
